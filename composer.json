{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.10",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "doctrine/collections": "^1.5",
        "eko/feedbundle": "^2.0",
        "geoip2/geoip2": "~2.0",
        "geokit/geokit": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^2.4",
        "nesbot/carbon": "^2.0.0",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "mockery/mockery": "^1.2",
        "overtrue/phplint": "^2.0",
        "phpmd/phpmd": "^2.7",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "phpstan/phpstan-symfony": "^0.12.0",
        "sebastian/phpcpd": "^6.0",
        "sensiolabs-de/deptrac-shim": "^0.8.2",
        "sensiolabs/security-checker": "^6.0",
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.1",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.6",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check": [
            "@phpunit",
            "@phpstan",
            "@phpmd",
            "@phpcs",
            "@phpcbf",
            "@phplint",
            "@phpcpd",
            "@deptrac",
            "@security-checker"
        ],
        "phpunit": "simple-phpunit",
        "phpstan": "phpstan analyse src --level=2",
        "phpmd": "phpmd src ansi ./ruleset.xml",
        "phpcs": "phpcs src",
        "phpcbf": "phpcbf src",
        "phplint": "phplint",
        "phpcpd": "phpcpd src",
        "deptrac": "deptrac",
        "security-checker": "security-checker security:check"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "id": "01D25P5RK3ZACFWN982T41YK23"
        }
    }
}
